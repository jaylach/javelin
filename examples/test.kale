object @user_profile
  -- Tags array
  array tags
    object $this
      property "label" as 
        return id + ': ' + label
      end
    end
  end

  -- Location
  object location
    property street
    property "geo" as
      return city + ', ' + state + ' ' + postal
    end
  end
end

object @user(key=false)
  property name(key="user_name")
end