import './address' as address

{
  id: {{userId}},
  userName: {{userName}},
  fullName: {{firstName}} + {{lastName}},
  homePhone: {{ phones | filter: { type: 'home' }
                       | first
                       | pluck: 'number' }},
  mobilePhone: {{ phones | filter: { type: 'mobile' }
                         | first
                         | pluck: 'number' }},
  address: {{ _ | address }}
}